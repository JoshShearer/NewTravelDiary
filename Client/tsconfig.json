{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./",
    "paths": {
      "#src/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "./src/**/*.ts", "./src/**/*.tsx", "src/Pages/_app.js", "src/Pages/index.js", "src/Comps/Map/MyGoogleMap/index.tsx", "src/models/Location/index.js", "src/Comps/Map/BaseMap/index.jsx"],
  "exclude": ["node_modules", "creditor", "test"]
}
